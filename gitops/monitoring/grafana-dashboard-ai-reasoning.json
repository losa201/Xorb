{
  "dashboard": {
    "id": null,
    "title": "Xorb AI Reasoning Components",
    "tags": ["xorb", "ai", "reasoning"],
    "style": "dark",
    "timezone": "browser",
    "editable": true,
    "hideControls": false,
    "graphTooltip": 0,
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": ["5s", "10s", "30s", "1m", "5m", "15m", "30m", "1h", "2h", "1d"],
      "time_options": ["5m", "15m", "1h", "6h", "12h", "24h", "2d", "7d", "30d"]
    },
    "refresh": "30s",
    "schemaVersion": 16,
    "version": 1,
    "links": [],
    "panels": [
      {
        "id": 1,
        "title": "Introspective Reasoning Performance",
        "type": "graph",
        "span": 6,
        "targets": [
          {
            "expr": "rate(xorb_introspective_reflections_total[5m])",
            "legendFormat": "Reflections/sec",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, rate(xorb_introspective_reflection_duration_seconds_bucket[5m]))",
            "legendFormat": "95th percentile latency",
            "refId": "B"
          }
        ],
        "yAxes": [
          {
            "label": "Rate / Latency",
            "show": true
          }
        ],
        "xAxes": [
          {
            "show": true
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 0
        }
      },
      {
        "id": 2,
        "title": "Temporal Memory Usage",
        "type": "graph",
        "span": 6,
        "targets": [
          {
            "expr": "xorb_temporal_memory_episodic_size",
            "legendFormat": "Episodic Memory Size",
            "refId": "A"
          },
          {
            "expr": "xorb_temporal_memory_semantic_size",
            "legendFormat": "Semantic Memory Size",
            "refId": "B"
          },
          {
            "expr": "xorb_temporal_memory_fragmentation_ratio * 100",
            "legendFormat": "Fragmentation %",
            "refId": "C"
          }
        ],
        "yAxes": [
          {
            "label": "Count / Percentage",
            "show": true
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 0
        }
      },
      {
        "id": 3,
        "title": "Hierarchical Policy Switching",
        "type": "graph",
        "span": 12,
        "targets": [
          {
            "expr": "rate(xorb_hierarchical_policy_switches_total[5m]) by (from_level, to_level)",
            "legendFormat": "{{from_level}} -> {{to_level}}",
            "refId": "A"
          }
        ],
        "yAxes": [
          {
            "label": "Switches/sec",
            "show": true
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 24,
          "x": 0,
          "y": 8
        }
      },
      {
        "id": 4,
        "title": "Root Cause Analysis Effectiveness",
        "type": "singlestat",
        "span": 4,
        "targets": [
          {
            "expr": "histogram_quantile(0.5, rate(xorb_root_cause_confidence_bucket[10m]))",
            "refId": "A"
          }
        ],
        "valueName": "current",
        "format": "percentunit",
        "thresholds": "0.3,0.7",
        "colorBackground": true,
        "gridPos": {
          "h": 4,
          "w": 8,
          "x": 0,
          "y": 16
        }
      },
      {
        "id": 5,
        "title": "Uncertainty Governance Decisions",
        "type": "graph",
        "span": 8,
        "targets": [
          {
            "expr": "rate(xorb_uncertainty_governance_decisions_total[5m]) by (decision_type)",
            "legendFormat": "{{decision_type}}",
            "refId": "A"
          }
        ],
        "yAxes": [
          {
            "label": "Decisions/sec",
            "show": true
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 16,
          "x": 8,
          "y": 16
        }
      },
      {
        "id": 6,
        "title": "Attack Chain Simulation Stats",
        "type": "table",
        "span": 12,
        "targets": [
          {
            "expr": "xorb_attack_simulation_generated_chains_total",
            "legendFormat": "Total Chains Generated",
            "refId": "A",
            "format": "table",
            "instant": true
          },
          {
            "expr": "histogram_quantile(0.5, rate(xorb_attack_chain_length_bucket[1h]))",
            "legendFormat": "Median Chain Length",
            "refId": "B",
            "format": "table",
            "instant": true
          },
          {
            "expr": "rate(xorb_attack_simulation_generation_failures_total[1h]) * 3600",
            "legendFormat": "Failures/hour",
            "refId": "C",
            "format": "table",
            "instant": true
          }
        ],
        "styles": [
          {
            "alias": "Metric",
            "colorMode": null,
            "colors": [],
            "dateFormat": "YYYY-MM-DD HH:mm:ss",
            "decimals": 2,
            "pattern": "Metric",
            "thresholds": [],
            "type": "string",
            "unit": "short"
          },
          {
            "alias": "Value",
            "colorMode": "value",
            "colors": ["rgba(245, 54, 54, 0.9)", "rgba(237, 129, 40, 0.89)", "rgba(50, 172, 45, 0.97)"],
            "decimals": 2,
            "pattern": "Value",
            "thresholds": ["10", "50"],
            "type": "number",
            "unit": "short"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 24,
          "x": 0,
          "y": 24
        }
      },
      {
        "id": 7,
        "title": "AI Component Resource Utilization",
        "type": "graph",
        "span": 12,
        "targets": [
          {
            "expr": "rate(container_cpu_usage_seconds_total{pod=~\".*ai-reasoning.*\"}[5m]) * 100",
            "legendFormat": "CPU Usage %",
            "refId": "A"
          },
          {
            "expr": "container_memory_usage_bytes{pod=~\".*ai-reasoning.*\"} / container_spec_memory_limit_bytes{pod=~\".*ai-reasoning.*\"} * 100",
            "legendFormat": "Memory Usage %",
            "refId": "B"
          }
        ],
        "yAxes": [
          {
            "label": "Percentage",
            "show": true,
            "max": 100
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 24,
          "x": 0,
          "y": 32
        }
      }
    ]
  }
}