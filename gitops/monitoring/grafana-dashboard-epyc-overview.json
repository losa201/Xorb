            "mappings": [
              {
                "options": {
                  "performance": {
                    "color": "red",
                    "index": 0,
                    "text": "Performance"
                  },
                  "ondemand": {
                    "color": "yellow",
                    "index": 1,
                    "text": "On-Demand"
                  },
                  "powersave": {
                    "color": "green",
                    "index": 2,
                    "text": "Power Save"
                  }
                },
                "type": "value"
              }
            ],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "green", "value": null}
              ]
            }
          }
        }
      },
      {
        "id": 5,
        "title": "EPYC L3 Cache Efficiency",
        "type": "gauge",
        "targets": [
          {
            "expr": "avg_over_time(epyc_l3_cache_hit_ratio[5m])",
