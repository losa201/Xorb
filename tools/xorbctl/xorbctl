#!/usr/bin/env python3
"""
XORB CLI entry point script.
This script can be symlinked or copied to /usr/local/bin for global access.
"""

import sys
import os
from pathlib import Path

# Add the CLI directory to Python path
cli_dir = Path(__file__).parent
sys.path.insert(0, str(cli_dir))

# Import and run the CLI
from cli import cli

if __name__ == "__main__":
    sys.exit(cli())