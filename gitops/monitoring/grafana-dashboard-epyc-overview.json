{
  "dashboard": {
    "id": null,
    "title": "Xorb 2.0 EPYC Performance Overview",
    "tags": ["xorb", "epyc", "performance"],
    "style": "dark",
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "EPYC CCX Utilization Heatmap",
        "type": "heatmap",
        "targets": [
          {
            "expr": "epyc_ccx_utilization_percent",
            "legendFormat": "CCX {{ccx}}",
            "refId": "A"
          }
        ],
        "gridPos": {"h": 9, "w": 12, "x": 0, "y": 0},
        "options": {
          "calculate": false,
          "cellGap": 2,
          "cellValues": {},
          "color": {
            "exponent": 0.5,
            "fill": "dark-orange",
            "mode": "spectrum",
            "reverse": false,
            "scale": "exponential",
            "scheme": "Spectral",
            "steps": 64
          },
          "exemplars": {
            "color": "rgba(255,0,255,0.7)"
          },
          "filterValues": {
            "le": 1e-9
          },
          "legend": {
            "show": false
          },
          "rowsFrame": {
            "layout": "auto"
          },
          "tooltip": {
            "show": true,
            "yHistogram": false
          },
          "yAxis": {
            "axisPlacement": "left",
            "reverse": false,
            "unit": "short"
          }
        },
        "fieldConfig": {
          "defaults": {
            "custom": {
              "hideFrom": {
                "legend": false,
                "tooltip": false,
                "vis": false
              },
              "scaleDistribution": {
                "type": "linear"
              }
            }
          }
        }
      },
      {
        "id": 2,
        "title": "NUMA Memory Locality Ratio",
        "type": "stat",
        "targets": [
          {
            "expr": "avg(epyc_numa_memory_locality_ratio)",
            "legendFormat": "Memory Locality",
            "refId": "A"
          }
        ],
        "gridPos": {"h": 9, "w": 12, "x": 12, "y": 0},
        "options": {
          "colorMode": "value",
          "graphMode": "area",
          "justifyMode": "auto",
          "orientation": "horizontal",
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          },
          "textMode": "auto"
        },
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "mappings": [],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "red", "value": null},
                {"color": "yellow", "value": 0.7},
                {"color": "green", "value": 0.85}
              ]
            },
            "unit": "percentunit",
            "min": 0,
            "max": 1
          }
        }
      },
      {
        "id": 3,
        "title": "DQN Agent Selection Performance",
        "type": "timeseries",
        "targets": [
          {
            "expr": "rate(xorb_dqn_agent_selections_total[5m])",
            "legendFormat": "Selections/sec",
            "refId": "A"
          },
          {
            "expr": "xorb_dqn_selection_accuracy_ratio",
            "legendFormat": "Accuracy",
            "refId": "B"
          },
          {
            "expr": "xorb_dqn_q_value_convergence",
            "legendFormat": "Q-Value Convergence",
            "refId": "C"
          }
        ],
        "gridPos": {"h": 9, "w": 24, "x": 0, "y": 9},
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "custom": {
              "axisLabel": "",
              "axisPlacement": "auto",
              "barAlignment": 0,
              "drawStyle": "line",
              "fillOpacity": 10,
              "gradientMode": "none",
              "hideFrom": {
                "legend": false,
                "tooltip": false,
                "vis": false
              },
              "lineInterpolation": "linear",
              "lineWidth": 1,
              "pointSize": 5,
              "scaleDistribution": {
                "type": "linear"
              },
              "showPoints": "never",
              "spanNulls": false,
              "stacking": {
                "group": "A",
                "mode": "none"
              },
              "thresholdsStyle": {
                "mode": "off"
              }
            },
            "mappings": [],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "green", "value": null},
                {"color": "red", "value": 80}
              ]
            },
            "unit": "short"
          }
        },
        "options": {
          "legend": {
            "calcs": [],
            "displayMode": "list",
            "placement": "bottom"
          },
          "tooltip": {
            "mode": "single"
          }
        }
      },
      {
        "id": 4,
        "title": "EPYC CPU Governor Status",
        "type": "table",
        "targets": [
          {
            "expr": "epyc_cpu_governor_current",
            "legendFormat": "{{governor}}",
            "refId": "A",
            "format": "table"
          }
